{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/hero-blocks",
	"version": "0.1.0",
	"title": "Hero Blocks",
	"category": "widgets",
	"icon": "cover-image",
	"description": "Hero block WHW",
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"selector": ".callout-title"
		},
		"mediaID": {
			"type": "number"
		},
		"mediaURL": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		},
		"height":{
			"type": "boolean"
		}
	},
	"textdomain": "hero-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
