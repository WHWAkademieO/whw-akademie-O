{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/text-cta-blocks",
	"version": "0.1.0",
	"title": "Text CTA Blocks",
	"category": "widgets",
	"icon": "button",
	"description": "Text CTA block WHW",
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"selector": ".callout-title"
		},
		"content": {
			"type": "string",
			"selector": ".callout-content"
		},
		"dot":{
			"type": "boolean"
		},
		"listCta": {
			"type": "array",
			"default": []
		}
	},
	"textdomain": "text-cta-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
